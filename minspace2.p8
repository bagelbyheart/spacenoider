function dropent(x,y)
 local e=genent(x,y)
 local d={
  l=2,
  faction=2,
  mov=_bounceround,
  dra=function(self)
   if self.f>100 then
    if self.f%2==0 then
     _pswap()
    end
   end
   spr(self.spr[1],self.x,
       self.y)
   pal()
  end
 }
e=_tmrg(e,d)
 return e
end

