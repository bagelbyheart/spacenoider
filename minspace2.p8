-- this function handles spawn
-- of new enemies over time.
-- normally it would be called
-- on page 4.
function waves()
  if f%150==0 then
   wave+=1
-- make elites more likely
-- 90   - 1*5/2 = 87.5
-- 87.5 - 2*5/2 = 82.5
-- 82.5 - 3*5/2 = 75
-- elitecut can probably be
-- removed and directly ref'd
-- in spawn routine.
   elitecut-=(wave*5)/2
   local rand=rnd(1)
-- 15% to spawn bombers.
   if rand>=.85 then
    _sline(4,bomber,10,10)
-- 35% to spawn gunners.
   elseif rand>.5 then
    _sline(4,gunner,10,10,'r')
-- 50% to spawn drillers.
   else
    _dline(8,driller,10,10)
   end
  end
end

function wavetest()
 if f%150==0 then
  wave+=1
  _sline(driller,4)
 end
end

