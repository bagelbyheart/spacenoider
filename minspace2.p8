function emake(en,x,y,more)
 local e=_tcpy(en.temp(x,y))
 e=_tmrg(e,en)
 if more then
  e=_tmrg(e,more)
 end
 e:ebhv()
 if e.test then
  return e
 else
  if limitmake(e) then
   add(entities,e)
   sfx(e.fsfx)
  end
 end
end

