-- > control functions
-- > spawn functions
function limitmake(ent)
 if ent.lim then
  local l=0;
  for e in all(entities) do
   if e.n==ent.n and
      e.faction==ent.faction then
    l+=1
   end
  end
  if l<ent.lim then
   return true
  else
   return false
  end
 else
  return true
 end
end

function _sline(n,en,x,y,side,more)
-- e = an entity function.
-- n = number of e to spawn.
 fullent=emake(en,x,y,{test=0})
 local xoff=fullent.w*1.5
 if side and side=="r" then
  xoff=-xoff
  x=128-x
 end
 for i=1,n do
  local ox=x+i*xoff
  local oy=y
  emake(en,ox,oy,more)
 end
end

function _dline(n,en,x,y,side,more)
 fullent=emake(en,x,y,{test=0})
 local xoff=fullent.w*1.5
 local yoff=-fullent.h*1.5
 if side then
  if side=="r" then
   xoff=-xoff
   x=128-x
  end
 end
 for i=1,n do
  local ox=x+i*xoff
  local oy=y+i*yoff
  emake(en,ox,oy,more)
 end
end

-- > movement functions

function _falldown(self)
 self.yd=self.spd
if self.faction==1 then
 if self.yd>=0 then
  self.yd=-self.yd
 end
end
 self.y+=self.yd
-- if the entity goes a ways
-- past the bottom of the
-- screen it gets deleted.
 if self.y > 150 then
  del(entities,self)
 end
end

function _bounceround(self)
 if self.xd==0 and
    self.yd==0 then
  self.xd=drnd()
  self.yd=drnd()
 end
 if self.f>=150 then
  del(entities,self)
 end
 local ly=16 uy=100
 if self.y > uy or
    self.y < ly then
  self.yd=-self.yd
 end
 local lx=16 ux=112
 if self.x > ux or
    self.x < lx then
  self.xd=-self.xd
 end
 self.x=mid(self.x,lx,ux)
 self.y=mid(self.y,ly,uy)
 self.x+=self.xd
 self.y+=self.yd
end

function _sideside(self)
 self.xd=self.spd
 if flr(self.f/30%2)==0 then
  self.xd=-self.xd
 end
 self.x+=self.xd
end

function _circles(self)
 if self.mx==0 then
  self.mx=self.x
  self.my=self.y
 end
   local lx=10 ux=118
   if self.x > ux then
    self.x=ux-(self.x-ux)
   end
   if self.x < lx then
    self.x=lx+(lx-self.x)
   end
   self.x=self.r*
          sin(self.f/50)+
          self.mx
   self.y=self.r*
          cos(self.f/50)+
          self.my
end

function playmov(self)
 local ly=10 uy=97
 if self.y > uy then
  self.y=uy end
 if self.y < ly then
  self.y=ly end
 local lx=0 ux=120
 if self.x > ux then
  self.x=ux end
 if self.x < lx then
  self.x=lx end
 _mobj(self)
end

